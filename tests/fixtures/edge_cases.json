      "input": {
        "emoji": "üî•üíØ‚ú®",
        "chinese": "‰∏≠ÊñáÊµãËØï",
        "japanese": "Êó•Êú¨Ë™û„ÉÜ„Çπ„Éà",
        "korean": "ÌïúÍµ≠Ïñ¥ ÌÖåÏä§Ìä∏",
        "arabic": "ÿßÿÆÿ™ÿ®ÿßÿ± ÿπÿ±ÿ®Ÿä",
        "russian": "–†—É—Å—Å–∫–∏–π —Ç–µ—Å—Ç",
        "mixed": "Hello‰∏ñÁïåüåç–ü—Ä–∏–≤–µ—Ç"
      },
      "expected_features": ["unicode support", "multi-language", "emoji"]
    },
    {
      "name": "numeric_precision",
      "input": {
        "smallFloat": 0.000001,
        "largeFloat": 123456789.123456789,
        "scientificPositive": 1.23e100,
        "scientificNegative": 1.23e-100,
        "integer": 9007199254740991,
